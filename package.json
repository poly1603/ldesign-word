{
  "name": "word-viewer",
  "version": "1.0.0",
  "private": true,
  "description": "功能强大的 Word 文档查看和编辑插件，支持多种框架",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w @word-viewer/core",
    "build:vue": "npm run build -w @word-viewer/vue",
    "build:react": "npm run build -w @word-viewer/react",
    "build:lit": "npm run build -w @word-viewer/lit",
    "dev": "npm run dev --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/word-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/word-viewer/issues"
  },
  "homepage": "https://github.com/your-username/word-viewer#readme",
  "keywords": [
    "word",
    "docx",
    "doc",
    "viewer",
    "editor",
    "document",
    "vue",
    "react",
    "lit",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "docx-preview": "^0.3.0",
    "mammoth": "^1.6.0",
    "docx": "^8.5.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "lit": "^3.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.1.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "lit": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "lit": {
      "optional": true
    }
  }
}

